{
  "name": "motor-cms/motor-core",
  "description": "Motor base package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.8.*",
    "predis/predis": "^1.1",
    "orchestra/testbench": "^3.8"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "ext-fileinfo": "*",
    "phpunit/phpunit": "^6.1|^7.5"
  },
  "autoload": {
    "psr-4": {
      "Motor\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Motor\\Core\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text --coverage-clover=coverage.xml"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Core\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
