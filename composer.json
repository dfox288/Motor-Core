{
  "name": "motor-cms/motor-core",
  "description": "Motor base package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^7.0",
    "laravel/tinker": "^2.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Motor\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Motor\\Core\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text --coverage-clover=coverage.xml"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Core\\Providers\\MotorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
